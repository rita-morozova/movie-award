ons.length === 5){\n    return(\n       <Banner />\n    )\n  }\n}\n \nrender(){\nconst {movies, nominations,  disabledButtons} = this.state\n  console.log(this.state.nominations)\n  return (\n    <div className=\"App\">\n      <SearchBar onChange={this.handleSearch} onSubmit={this.handleSubmit} findMovies={this.findMovies} />\n      <ResultsContainer  movies={movies}  addToNomination={this.addToNomination}  disabledButtons={disabledButtons} />\n      <NominationsContainer nominations={nominations} removeFromNomination={this.removeFromNomination}/>\n      {this.displayBanner()}\n    </div>\n  )\n }\n}\n\nexport default App;\n","/Users/Rita/Desktop/Flatiron/success/shoppies/src/SearchBar.js",[],"/Users/Rita/Desktop/Flatiron/success/shoppies/src/index.js",[],["38","39"],"/Users/Rita/Desktop/Flatiron/success/shoppies/src/ResultsContainer.js",[],["40","41"],"/Users/Rita/Desktop/Flatiron/success/shoppies/src/MovieCard.js",[],"/Users/Rita/Desktop/Flatiron/success/shoppies/src/NominationsContainer.js",["42"],"/Users/Rita/Desktop/Flatiron/success/shoppies/src/NominatedMovie.js",[],"/Users/Rita/Desktop/Flatiron/success/shoppies/src/Banner.js",[],{"ruleId":"43","severity":1,"message":"44","line":1,"column":16,"nodeType":"45","messageId":"46","endLine":1,"endColumn":24},{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"47","replacedBy":"51"},{"ruleId":"49","replacedBy":"52"},{"ruleId":"43","severity":1,"message":"53","line":3,"column":8,"nodeType":"45","messageId":"46","endLine":3,"endColumn":14},"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","no-native-reassign",["54"],"no-negated-in-lhs",["55"],["54"],["55"],"'Banner' is defined but never used.","no-global-assign","no-unsafe-negation"]